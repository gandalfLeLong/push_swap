# -----------------------------------Makefile------------------------------------
NAME	= push_swap

DEBUG	= debug.out
# -----------------------------------Sources-------------------------------------

HEADER	= includes/pushswap.h

SRCS	= main.c \
		  parsing.c \
		  list_utils.c \
		  list_utils_2.c \
		  swap_instructions.c \
		  push_instructions.c \
		  rotate_instructions.c \
		  reverse_rotate_instructions.c \
		  sorting_algorithm.c \
		  sorting_algorithm_2.c \
		  sorting_algorithm_3.c \

SRC		= ${addprefix sources/, ${SRCS}}

OBJS	= ${SRC:.c=.o}

# -----------------------------------Compilation---------------------------------

CC			= cc

FLAGS		= -Wall -Wextra -Werror

RM			= rm -f

FSANITIZE	= -fsanitize=address -g

# -----------------------------------Libraries-----------------------------------

FT_FOLDER	= libft

FT			= ${FT_FOLDER}/libft.a

MAKE_FT		= make -s -C ${FT_FOLDER}

INCLUDES	= -I ${FT_FOLDER}/includes \
			  -I ./includes

LIBRARIES	= -L./${FT_FOLDER} -lft

# -----------------------------------Rules---------------------------------------

all: ${NAME}

${NAME}: ${FT} ${OBJS} ${HEADER}
		${CC} ${FLAGS} ${OBJS} ${LIBRARIES} -o ${NAME} 

${FT}:
	${MAKE_FT}

%.o: %.c
	${CC} ${FLAGS} -c ${INCLUDES} $< -o $@

debug: ${FT} ${OBJS} ${HEADER}
	${CC} ${FLAGS} ${FSANITIZE} ${OBJS} ${LIBRARIES} -o ${DEBUG}

re: fclean all

clean:
		@${RM} ${OBJS}
		@${MAKE_FT} clean
		@echo "It's clean."

fclean: clean
		@${RM} ${NAME}
		@${RM} ${DEBUG}
		@${MAKE_FT} fclean
		@echo "It's like mega clean mate."

# -----------------------------------Fun_Rules----------------------------------

